# Triggerfish LSP Configuration

# Logging
TRIGGERFISH_LOG_LEVEL=INFO
TRIGGERFISH_LOG_FILE=~/.triggerfish/logs/triggerfish.log

# CTags
TRIGGERFISH_CTAGS_EXECUTABLE=ctags
TRIGGERFISH_CTAGS_TIMEOUT=30

# Completion
TRIGGERFISH_MIN_FUZZY_SCORE=60
TRIGGERFISH_MAX_COMPLETION_ITEMS=50

# Go Core Subprocess
TRIGGERFISH_CORE_ENABLED=1
TRIGGERFISH_CORE_EXECUTABLE=triggerfish-core
TRIGGERFISH_CORE_TIMEOUT=10
